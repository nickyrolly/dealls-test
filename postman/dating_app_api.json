{
	"info": {
		"_postman_id": "1234567890",
		"name": "Dating App API",
		"description": "API collection for the Dating App",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/",
					"host": ["{{base_url}}"],
					"path": [""]
				},
				"description": "Health check endpoint to verify API is running"
			}
		},
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Sign Up",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"user@example.com\",\n    \"password\": \"password123\",\n    \"first_name\": \"John\",\n    \"last_name\": \"Doe\",\n    \"date_of_birth\": \"1990-01-01\",\n    \"gender\": \"male\",\n    \"phone_number\": \"+1234567890\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/signup",
							"host": ["{{base_url}}"],
							"path": ["api", "signup"]
						},
						"description": "Register a new user"
					}
				},
				{
					"name": "Login",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Basic {{base64_credentials}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"user@example.com\",\n    \"password\": \"password123\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/login",
							"host": ["{{base_url}}"],
							"path": ["api", "login"]
						},
						"description": "Login and get authentication token"
					}
				}
			]
		},
		{
			"name": "Profile",
			"item": [
				{
					"name": "Get Profile",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/v1/profile",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "profile"]
						},
						"description": "Get user profile information"
					}
				},
				{
					"name": "Update Profile",
					"request": {
						"method": "PUT",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"first_name\": \"John\",\n    \"last_name\": \"Doe\",\n    \"bio\": \"I love hiking and photography\",\n    \"location_lat\": 40.7128,\n    \"location_lng\": -74.0060\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/profile",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "profile"]
						},
						"description": "Update user profile information"
					}
				},
				{
					"name": "Update Preferences",
					"request": {
						"method": "PUT",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"min_age\": 25,\n    \"max_age\": 35,\n    \"distance\": 50,\n    \"gender_preference\": \"female\",\n    \"interests\": [\"hiking\", \"photography\", \"travel\"]\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/profile/preferences",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "profile", "preferences"]
						},
						"description": "Update user preferences for matching"
					}
				}
			]
		},
		{
			"name": "Matches & Likes",
			"item": [
				{
					"name": "Get Matches",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/v1/matches",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "matches"]
						},
						"description": "Get user's matches"
					}
				},
				{
					"name": "Like User",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"user_id\": \"12345678-1234-5678-1234-567812345678\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/likes",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "likes"]
						},
						"description": "Like another user"
					}
				},
				{
					"name": "Withdraw Like",
					"request": {
						"method": "DELETE",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/v1/likes/{{user_id}}",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "likes", "{{user_id}}"]
						},
						"description": "Withdraw like from another user"
					}
				},
				{
					"name": "Unmatch User",
					"request": {
						"method": "DELETE",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/v1/matches/{{user_id}}",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "matches", "{{user_id}}"]
						},
						"description": "Unmatch from another user"
					}
				}
			]
		}
	],
	"variable": [
		{
			"key": "base_url",
			"value": "http://localhost:8080"
		},
		{
			"key": "auth_token",
			"value": "your_jwt_token_here"
		},
		{
			"key": "user_id",
			"value": "12345678-1234-5678-1234-567812345678"
		},
		{
			"key": "base64_credentials",
			"value": "base64_encoded_email_password"
		}
	]
}
